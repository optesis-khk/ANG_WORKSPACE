<odoo>

        <!-- Request-related subtypes for messaging / Chatter -->
        <record id="mt_request_to_approve" model="mail.message.subtype">
            <field name="name">supply Request to be approved</field>
            <field name="res_model">supply.request</field>
            <field name="default" eval="True"/>
            <field name="description">supply Request to be approved</field>
        </record>

        <record id="mt_request_approved" model="mail.message.subtype">
            <field name="name">supply Request approved</field>
            <field name="res_model">supply.request</field>
            <field name="default" eval="True"/>
            <field name="description">supply Request approved</field>
        </record>

        <record id="mt_request_rejected" model="mail.message.subtype">
            <field name="name">supply Request rejected</field>
            <field name="res_model">supply.request</field>
            <field name="default" eval="True"/>
            <field name="description">supply Request rejected</field>
        </record>

        <record id="mt_request_done" model="mail.message.subtype">
            <field name="name">supply Request done</field>
            <field name="res_model">supply.request</field>
            <field name="default" eval="True"/>
            <field name="description">supply Request is done</field>
        </record>

</odoo>
