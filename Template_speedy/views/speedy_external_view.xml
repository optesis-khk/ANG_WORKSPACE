<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <data>
        <template id="speedy_external_view">

            <t t-if="not company">
                <!-- Multicompany -->
                <t t-if="company_id">
                    <t t-set="company" t-value="company_id"/>
                </t>
                <t t-elif="o and 'company_id' in o">
                    <t t-set="company" t-value="res_company"/>
                </t>
                <t t-else="else">
                    <t t-set="company" t-value="res_company"/>
                </t>
            </t>

            <div class="header" style="margin-left:30px;padding-left:30px">
                <div class="row">
                    <div class="col-12" style=";margin-top:-10px;margin-left:35%;background-color:white">
                        <a href="#default">
                            <img src="/Template_speedy/static/img/speedy.png"
                                 style="width:20%; margin-bottom:2px; background-color:white"/>
                        </a>
                    </div>
                </div>
            </div>

            <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name"
                 t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
                <t t-call="web.address_layout"/>
                <t t-raw="0"/>
            </div>

            <div class="footer o_standard_footer">
                <p class="text-right" align="right">  <![CDATA[&nbsp;]]></p>
                  <img src="/Template_speedy/static/img/foot_with_link.png"
                                 style="width:100%;height:auto;margin-top:-28px;margin-bottom:5px"/>
                
               
            </div>
        </template>
    </data>
</odoo>