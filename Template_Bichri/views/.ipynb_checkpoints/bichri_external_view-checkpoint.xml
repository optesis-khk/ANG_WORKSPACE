<?xml version="1.0" encoding="UTF-8" ?>

<odoo>

    <data>

        <template id="bichri_external_view">



            <t t-if="not company">

                <!-- Multicompany -->

                <t t-if="company_id">

                    <t t-set="company" t-value="company_id"/>

                </t>

                <t t-elif="o and 'company_id' in o">

                    <t t-set="company" t-value="res_company"/>

                </t>

                <t t-else="else">

                    <t t-set="company" t-value="res_company"/>

                </t>

            </t>



             <div class="header">



                <div class="row">



                    <div class="col-12" style="padding-left:15px;margin-left:15px">

                            <img src="/Template_Bichri/static/img/logo.png"
                                 style="width:15%; margin-bottom:2px;"/> 

                    </div>



                </div>



            </div>

   <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name"
                 t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
                <t t-call="web.address_layout"/>
                <t t-raw="0"/>
    </div>





          



                <div class="footer o_standard_footer">



                <div class="row" style="margin-top:10%;">



             



                    <div class="col-12">



                        <div class="container" style=

                             "padding-right:-30px;">



                            <img src="/Template_Bichri/static/img/FOOTER.png" align="right"



                                 style=""/>



                     



                    </div>



                </div>



            </div>



            </div>

        </template>

    </data>

</odoo>