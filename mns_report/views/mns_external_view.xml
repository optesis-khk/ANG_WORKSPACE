<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <template id="mns_external_view">

            <t t-if="not company">
                <!-- Multicompany -->
                <t t-if="company_id">
                    <t t-set="company" t-value="company_id"/>
                </t>
                <t t-elif="o and 'company_id' in o">
                    <t t-set="company" t-value="res_company"/>
                </t>
                <t t-else="else">
                    <t t-set="company" t-value="res_company"/>
                </t>
            </t>

            <div class="header">
                <div class="row" align="right">
                    <div class="col-12">
                        <a href="#default">
                            <img src="/mns_report/static/src/images/mns_consulting_logo.jpg"
                                 style="width:50%; margin-bottom:2px;"/>
                        </a>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <a href="#default">
                            <img src="/mns_report/static/src/images/mns_color.png"
                                 style="width:100%; margin-bottom:2px;"/>
                        </a>
                    </div>
                </div>
                
            </div>

            <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name"
                 t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
                <t t-call="web.address_layout"/>
                <t t-raw="0"/>
            </div>

            <div class="footer">
                <div class="row" style="margin-bottom:7%;">
                    <div class="col-12">
                        <div class="container">
                            <img src="/mns_report/static/src/images/mns_footer_1.png" align="center"
                                 style=""/>
                        </div>
                    </div>
                </div>
            </div>
        </template>
    </data>
</odoo>